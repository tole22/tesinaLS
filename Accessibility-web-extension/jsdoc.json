{
    "source": {
        "include": ["./src"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "encoding": "utf8",
        "readme": "./README.MD",
        "recurse": "true",
        "destination": "./doc",
        "verbose": "true",
        "template": "node_modules/docdash",
        "tutorials": "./tutorials"
    }
}